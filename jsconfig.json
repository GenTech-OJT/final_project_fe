{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@pages/*": ["src/pages/*"],
      "@routes/*": ["src/routes/*"],
      "@constants/*": ["src/constants/*"],
      "@api/*": ["src/apis/*"],
      "@hooks/*": ["src/hooks/*"]

      // Thêm các alias khác ở đây
    }
  },
  "exclude": ["node_modules"]
}
